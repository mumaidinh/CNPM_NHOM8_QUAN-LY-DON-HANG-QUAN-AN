def bao_cao_mon_ban_chay():
    thong_ke = {}

    for don in don_hang_list:
        if don["trang_thai"] != "Ho√†n th√†nh":
            continue

        for mon in don.get("mon", []):
            thong_ke[mon] = thong_ke.get(mon, 0) + 1

    if not thong_ke:
        print("‚ö†Ô∏è Ch∆∞a c√≥ d·ªØ li·ªáu b√°n h√†ng!")
        return

    print("\nüî• M√ìN B√ÅN CH·∫†Y üî•")
    for mon, so_luong in sorted(thong_ke.items(), key=lambda x: x[1], reverse=True):
        print(f"{mon}: {so_luong} l·∫ßn")
